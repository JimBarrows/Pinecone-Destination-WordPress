{
  "name" : "pinecone-wordpress-destinations",
  "version" : "1.0.0",
  "description" : "RabbitMq wordpress subscribers",
  "main" : "index.js",
  "scripts" : {
	"destinations:start:dev" : "nodemon index.js --exec babel-node --presets es2015,stage-2",
	"destinations:docker:run:dev" : "docker run --detach --name pinecone-destinations --link pinecone-mongo:mongo --link pinecone-rabbitmq:rabbitmq registry.gitlab.com/reallybigtree/pinecone-destination",
	"destinations:docker:build:dev" : "docker build -t registry.gitlab.com/reallybigtree/pinecone-destination .",
	"destinations:docker:push:dev" : "docker push registry.gitlab.com/reallybigtree/pinecone-destination"
  },
  "author" : "Jim Barrows",
  "license" : "ISC",
  "dependencies" : {
	"amqplib" : "^0.4.2",
	"babel-cli" : "^6.10.1",
	"babel-core" : "^6.10.4",
	"babel-preset-es2015" : "^6.9.0",
	"babel-preset-stage-2" : "^6.11.0",
	"bluebird" : "^3.4.1",
	"facebook-node-sdk" : "^0.2.0",
	"moment" : "^2.14.1",
	"mongoose" : "^4.5.3",
	"nodemon" : "^1.9.2",
	"pinecone-models" : "git+ssh://git@gitlab.com/ReallyBigTree/Pinecone-Models.git#develop",
	"wordpress" : "^1.2.2"
  }
}
